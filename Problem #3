#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;
int main()
{
	double x, y, z, V;
	cout<<"Enter the value for x: "; cin>>x;
	cout<<"Enter the value for y: "; cin>>y;
	z = 2.5;
	
	switch (x==1 && y)
	{
		case 1:
		switch (x==1 && y>=5)
		{
			case 0:
			V = x*y*z;
			cout <<setw(10)<<fixed<<setprecision(2)<< "V = "<<V<<endl;
			break;
			
			case 1:
			V = x+y/z;
			cout <<setw(10)<<fixed<<setprecision(2)<< "V = "<<V<<endl;
			break;
		}
		break;
		
		case 0:
			switch (x==2 && y<=5)
			{
				case 1:
				V = abs((x-y)/z);
				cout <<setw(10)<<fixed<<setprecision(2)<< "V = "<<V<<endl;
				break;
		
				case 0:
				switch (x>2)
				{
				case 1:
				V = x+y+z;
				cout <<setw(10)<<fixed<<setprecision(2)<< "V = "<<V<<endl;
				break;
				
				case 0:
				V = x-sqrt(y+z);
				cout <<setw(10)<<fixed<<setprecision(2)<< "V = "<<V<<endl;
				break;	
				}	
				
			}
			
		break;
	}
system("pause");

return 0;
}
